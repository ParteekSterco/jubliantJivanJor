    <rewrite>
      <rewriteMaps>
        <rewriteMap name="StaticRewrites">

          <add key="/" value="/index.aspx" />

          <add key="/products" value="/products.aspx.aspx?mpgid=18&amp;pgidtrail=18" />

        </rewriteMap>
        <rewriteMap name="StaticRedirects">

          <add value="/" key="/index.aspx" />
          <add value="/products" key="/products.aspx.aspx?mpgid=18&amp;pgidtrail=18" />
         

        </rewriteMap>
