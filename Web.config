      <rules>
        
        <!--<rule name="RemoveDoubleSlash" patternSyntax="ECMAScript" stopProcessing="true">
          <match url="(.*)" />
          <action type="Redirect" url="{C:1}/{C:2}" />
          <conditions>
            <add input="{UNENCODED_URL}" pattern="(.*)//(.*)" />
          </conditions>
        </rule>-->

       
        
        <rule name="Rewrite Rule" stopProcessing="true">
          <match url=".*" />
          <conditions>
            <add input="{StaticRewrites:{URL}}" pattern="(.+)" />
          </conditions>
          <action type="Rewrite" url="{C:1}" appendQueryString="false" />
        </rule>
        <rule name="Redirect Rule" stopProcessing="true">
          <match url=".*" />
          <conditions>
            <add input="{StaticRedirects:{REQUEST_URI}}" pattern="(.+)" />
          </conditions>
          <action type="Redirect" url="{C:1}" appendQueryString="False" redirectType="Permanent" />
        </rule>

        <rule name="Rewrite for happenings-detail outer with param 2">
          <match url="^happening-detail/(.*)/(.*)$" />
          <action type="Rewrite" url="/news-details.aspx?mpgid=15&amp;pgidtrail=16&amp;ptitle={R:1}&amp;eventsid={R:2}" />
        </rule>
       
        <rule name="Rewrite for gallery-details outer with param 2">
          <match url="^gallery-detail/(.*)/(.*)$" />
          <action type="Rewrite" url="/gallery-photo-details.aspx?mpgid=15&amp;pgidtrail=17&amp;ptitle={R:1}&amp;albumid={R:2}" />
        </rule>
        
        <rule name="Rewrite for blog detail outer with param 2">
          <match url="^blogs/(.*)/(.*)$" />
          <action type="Rewrite" url="/blog/blog_details.aspx?ptitle={R:1}&amp;blogid={R:2}" />
        </rule>
      </rules>

    </rewrite>
